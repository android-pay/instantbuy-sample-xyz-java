<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>wallet-java-test</application>
  <version>1</version>
  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    <!-- existing environment values -->
    <property name="sandbox_merchant_id" value="Your sandbox merchant ID" />
    <property name="sandbox_merchant_auth_key" value="Your sandbox merchant auth key" />
    <!-- set values for oauth-->
    <property name="oauth_client_id"
      value="Your OAuth 2.0 Client ID" />
    <property name="oauth_api_key" value="Your OAuth 2.0 API key" />
    <!-- use SANDBOX or PRODUCTION and set the values below -->
    <property name="online_wallet_enviroment" value="SANDBOX" />
    <property name="production_merchant_id" value="Your production merchant ID" />
    <property name="production_merchant_auth_key" value="Your production merchant auth key" />
  </system-properties>
  <static-files>
    <include path="**/*.html" />
    <include path="**/*.js" />
    <include path="**/*.css" />
    <include path="**/*.png" />
    <include path="**/*.jpeg" />
    <include path="**/*.jpg" />
    <include path="**/*.gif" />
    <include path="**/*.psd" />
    <include path="**/*.ico" />
    <include path="**/*.woff" />
    <include path="**/*.svg" />
    <include path="**/*.ttf" />
    <include path="**/*.eot" />
    <include path="*.ico" />
    <include path="*.html" />
    <include path="*.js" />
  </static-files>
  <resource-files>
    <include path="css/**" />
    <include path="css/*" />
    <include path="css" />
    <include path="*.vm" />
  </resource-files>
  <threadsafe>true</threadsafe>
  <precompilation-enabled>true</precompilation-enabled>
  <sessions-enabled>true</sessions-enabled>
</appengine-web-app>
